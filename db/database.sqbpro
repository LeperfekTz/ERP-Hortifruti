CREATE TABLE vendas_nova (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nomeProd TEXT NOT NULL,
    quantidade INTEGER NOT NULL,
    valor_total REAL NOT NULL
);

-- Passo 2: Copiar dados da tabela antiga para a nova tabela
INSERT INTO vendas_nova (id, nomeProd, quantidade, valor_total)
SELECT id, (SELECT nome FROM produtos WHERE produtos.id = vendas.produto_id) AS nomeProd,
       quantidade, valor_total
FROM vendas;

-- Passo 3: Excluir a tabela antiga
DROP TABLE vendas;

-- Passo 4: Renomear a nova tabela para o nome da tabela antiga
ALTER TABLE vendas_nova RENAME TO vendas;
